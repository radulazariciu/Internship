@model IEnumerable<Samsung.Models.User>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.firstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.secondName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.zipCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cellPhone)
        </th>
        <th></th>
    </tr>

    @foreach (var modelItem in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(model => modelItem.firstName)
            </td>
            <td>
               @Html.DisplayFor(model => modelItem.secondName)
            </td>
            <td>
                @Html.DisplayFor(model => modelItem.email)
            </td>
            <td>
                @Html.DisplayFor(model => modelItem.password)
            </td>
            <td>
                @Html.DisplayFor(model => modelItem.zipCode)
            </td>
            <td>
                @Html.DisplayFor(model => modelItem.cellPhone)
            </td>
        </tr>
    }

</table>
